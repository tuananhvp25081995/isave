                                    </div>
                                </div>
                                <div class="form-group border-bottom row pb-3">
                                    <label for="group_id" class="col-sm-3 text-right control-label col-form-label">Group
                                        ID</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" id="group_id" placeholder="Group ID">
                                    </div>
                                </div>
                            </div>
                            <div class="card-body bg-light">
                                <div class="form-group mb-0 text-right">
                                    <button id="btn_configSave" class="btn btn-info waves-effect waves-light">Save</button>
                                    <button id="btn_configCancel" class="btn btn-dark waves-effect waves-light">Cancel</button>
                                </div>
                            </div>
